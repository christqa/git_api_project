                            </div>
                        </div>
                    </div>
                    <div class="progress">
                        <div class="progress-bar-small gray" style="width: 81%; margin-bottom: 7px;">{{ data.hydration.urineDuration.config.minimumLowerBound }} seconds
                        </div>
                        <div class="progress-bar-small gray" style="width: 11%; white-space: nowrap;">{{ data.hydration.urineDuration.config.maximumUpperBound }} seconds
                        </div>
                    </div>
                    <div class="progress">
                        <div class="progress-bar-small" style="width: 20%; background-color: #F9DEAE; "></div>
                        <div class="progress-bar-small" style="width:20%; background-color: #F5ADAD; "></div>
                        <div class="progress-bar-small" style="width:60%; background-color: #47D89A; "></div>
                    </div>
                    <div class="progress">
                        <div class="progress-bar-small gray" style="width: 20%; text-align: center; ">Low</div>
                        <div class="progress-bar-small gray " style="width:20%; text-align: center;   ">Fair</div>
                        <div class="progress-bar-small  " style=" width:60%;  text-align: center; ">Good</div>
                    </div>
                </td>
                <td class="td-width">
                    <div id="" style="position: relative; padding-top: 20px; ">
                        <canvas id="urineDuration" height="195"></canvas>
                    </div>
                </td>
            </tr>
        </table>
        {% endif %}
        <!--  Urine Duration End -->

        <!--  Frequency Details Start -->
        {% if data.frequencyDetails %}
        <table class="table-width">
            <div class="th-width" style="padding-top: 24px;">
                <img class="icon-img" style="height: 40px; width: 40px; " src="templates/images/frequency.svg" alt="image">
            </div>
            <div style="padding-top: 5px; margin-left: 48px;">
                <h2 style="margin-bottom: -15px;"> Frequency Details</h2>
                <p class="smallfont">{{ data.header.timePeriod }} Average</p>
            </div>
        </table>
        <div>
            <table class="table-width reactor" style="margin-top:10px;">
                <thead>
                    <tr class="table-border change">
                        <th class="table-border" style="text-align: center;">DATE</th>
                        <th class="table-border" style="text-align: center;">TIME</th>
                        <th class="table-border" style="text-align: center;">TYPE</th>
                        <!-- ANNOTATIONS - DISABLED
                        <th class="table-border" style="text-align: left;">ANNOTATIONS</th>
                        -->
                    </tr>
                </thead>
                {% for frequencydata in data.frequencyDetails %}
                <tr class="table-border change">
                    <td class="table-border" style="text-align: center;"> {{ frequencydata.date }} </td>
                    <td class="table-border" style="text-align: center;">
                        {%- assign frequencydataletter = frequencydata.time | slice: 6 -%} {% if frequencydataletter == "A" %}
                        <a> <img src="templates/images/sun.svg" style="height: 20px; width:20px;" alt="image"></a> {% endif %} {% if frequencydataletter == "P" %}
                        <a style="padding-left: 4px;"> <img src="templates/images/moon.svg" style="height: 15px; width:15px; " alt="image"></a> {% endif %} {{ frequencydata.time }}
                    </td>
                    <td class="table-border" style="text-align: center;">{{ frequencydata.type }}</td>
                    <!-- ANNOTATIONS - DISABLED
                    <td class="table-border" style="text-align: left; ">{{ frequencydata.annotation }}</td>
                    -->
                </tr>
                {% endfor %}
            </table>
        </div>
        {% endif %}
        <!--  Frequency Details End -->
    </div>
</body>

</html>